generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Quote {
  id     String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  author String
  genre  String
  quote  String
}
